{
  "name": "sugtext",
  "version": "0.0.1",
  "description": "Text completion addon for the browser",
  "scripts": {
    "build": "npm run build-background & npm run build-content-script",
    "watch": "npm run watch-background & npm run watch-content-script",
    "build-content-script": "browserify js/index.js -o dist/main.js",
    "build-background": "browserify js/background.js -o dist/background.js",
    "watch-content-script": "watchify --debug js/index.js -o dist/main.js",
    "watch-background": "watchify --debug js/background.js -o dist/background.js",
    "test:e2e": "tape ./test/e2e/*.spec.js | tap-spec",
    "test": "standard && tape ./test/unit/*.spec.js | tap-spec",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "author": "Aram Drevekenin <grimsniffer@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "aliasify": "^2.1.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "looks-same": "^3.3.0",
    "proxyquire": "^2.0.1",
    "puppeteer": "^1.5.0",
    "sinon": "^6.1.4",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.1",
    "watchify": "^3.11.0"
  },
  "dependencies": {
    "@redom/store": "~1.0.0",
    "caret-pos": "^1.2.0",
    "debounce": "^1.2.0",
    "fluture": "^8.0.2",
    "insert-text-at-cursor": "^0.1.1",
    "monet": "^0.8.10",
    "pouchdb": "^7.0.0",
    "pouchdb-collate": "^7.0.0",
    "pouchdb-debug": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "ramda": "^0.25.0",
    "redom": "~3.12.3",
    "uuid": "^3.3.2"
  }
}
