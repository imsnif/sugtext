{
  "manifest_version": 2,
  "name": "sugtext",
  "version": "1.4",
  "description": "Autocompletes text in textAreas",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/main.js"],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": ["dist/background.js"]
  },
  "icons": {
    "48": "assets/sugtext-logo-48.png",
    "96": "assets/sugtext-logo-96.png"
  },
  "applications": {
    "gecko": {
      "id": "{8580352b-4e45-4e4d-a6ae-b3b87c9d9aed}",
      "strict_min_version": "42.0"
    }
  }
}
