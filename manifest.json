{
  "manifest_version": 2,
  "name": "sugtext",
  "version": "1.0",
  "description": "Autocompletes text in textAreas",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/main.js"],
      "all_frames": true
    }
  ],
  "background": {
    "scripts": ["dist/background.js"]
  }
}
